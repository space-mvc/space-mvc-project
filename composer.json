{
    "name": "space-mvc/space-mvc",
    "type": "project",
    "description": "This is the space mvc framework",
    "keywords": [
        "Space Mvc",
        "Framework"
    ],
    "version" : "0.0.1",
    "homepage": "https://github.com/space-mvc/space-mvc",
    "license": "MIT",
    "authors": [
        {
            "name": "Space MVC",
            "email": "space-mvc@gmail.com",
            "homepage": "http://space-mvc.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "~8.0",
        "filp/whoops": "^2.12",
        "robmorgan/phinx": "^0.12.6",
        "space-mvc/space-mvc-active-record": "dev-master",
        "space-mvc/space-mvc-framework": "dev-main"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=8.0"
    },
    "repositories": [
      {
        "type": "git",
        "url": "https://github.com/space-mvc/space-mvc-framework"
      },
      {
        "type": "vcs",
        "url": "https://github.com/space-mvc/space-mvc-active-record"
      }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "app"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
